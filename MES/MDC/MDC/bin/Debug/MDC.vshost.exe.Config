<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net"/>
  </configSections>
  <appSettings>
    <!--********************************************************************************************-->
    <!--****************************************通用配置********************************************-->
    <!--********************************************************************************************-->
    <add key="DbLog" value=",SqlLog,WorkLog,ErrorLog,LoginLog,HttpErrorLog," />
    <!--操作完成时,不同的状态是否提示音乐，Success Fail Warning 三种状态可选，不同状态间用逗号","分隔-->
    <add key="PlaySuccess" value="Fail,Warning"/>
    <!--设备编码-->
    <add key="mouldcode" value=""/>
    <!--数据上传服务器IP地址-->
    <!--<add key="HBase_Interface_IP" value=""/>-->
    <add key="HBase_Interface_IP" value="172.20.23.232"/>
    <!--HBase数据库IP地址-->
    <!--<add key="HBase_DataServer_IP" value=""/>-->
    <add key="HBase_DataServer_IP" value="172.20.23.231"/>
    <!--玻璃信息查询超时时间-->
    <add key="SearchTimeout" value="10000"/>
    <!--最大日志行数（超过将自动清空）-->
    <add key="MaxLogCount" value="200"/>
    <!--是否记录程序运行日志(True/False)-->
    <add key="WriteAPPLog" value="True"/>
    <!--HBase数据查询重试次数-->
    <add key="RetryCount" value="3" />
    <!--条码中是否允许小写字母（True：允许，False：禁止）-->
    <add key="LowerEnabled" value="False" />
    
    
    <!--********************************************************************************************-->
    <!--************************************自动过站程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--编码绑定超时时间（毫秒）-->
    <add key="BindingTimeout" value="1000"/>
    <!--玻璃检测信号过滤时间(毫秒，与上一片玻璃检测信号的时间间隔少于此设定值，将被丢弃）-->
    <add key="LCDFilter" value="3000"/>
    <!--不良报警阈值（单位:片，近50片玻璃中的不良数大于此设定值将播放报警音）-->
    <add key="WarningValue" value="10"/>
    
    <!--********************************************************************************************-->
    <!--************************************不良复判程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--复测AOI不良图片路径-->
    <add key="AOI_NG_Path" value=""/>
    <!--是否展示复测AOI不良图片-->
    <add key="AOI_NG_Enabled" value="True"/>

    <!--********************************************************************************************-->
    <!--************************************自动包装程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--包装串口名称-->
    <add key="PackagePort" value="COM2"/>
    
    <!--********************************************************************************************-->
    <!--************************************产线投料程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--切换工单接口地址-->
    <add key="OrderSwitchUrl" value="http://172.16.1.2:6605/Service/ServiceHandler.ashx"/>

    <!--********************************************************************************************-->
    <!--************************************物流看板程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--数据刷新周期（单位：毫秒）-->
    <add key="RefreshTime" value="60000"/>
    <!--图表轮播间隔时间（单位：毫秒）-->
    <add key="PlayInterval" value="30000"/>
    
    <!--********************************************************************************************-->
    <!--************************************华为上传程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--配置参数组-->
    <add key="ConfigGroup" value="official"/>
    <add key="HWCusNumber" value="23020712,23020730,23020737,23020909"/>

    <!--********************************************************************************************-->
    <!--************************************读转喷程序配置****************************************-->
    <!--********************************************************************************************-->
    <!--喷码机串口-->
    <add key="KGKSerialPort" value="COM1"/>
    <!--是否启用玻璃检测-->
    <add key="GlassDetectorEnabled" value="False"/>
    <!--玻璃检测传感器串口-->
    <add key="GlassDetectorPort" value="COM2"/>
    <!--扫码头类型：1=基恩士网口连接，2=康耐视网口连接，3=康耐视串口连接-->
    <add key="ReaderType" value="2"/>
    
  </appSettings>
  <connectionStrings>
    <add name="Base" connectionString="Data Source=172.20.23.230\SQLSERVER;User ID=sa;Password=Skyworth.2020;Initial Catalog=YJ_CWMES;Pooling=true;Connection Timeout=3" />
  </connectionStrings>
</configuration>